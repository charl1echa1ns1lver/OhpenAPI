<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="AutomationDemoTests">
	<listeners>
		<listener class-name="framework.report.ReportListener"/>
	    <listener class-name="framework.test.ExecutionListener"/>
	</listeners>

    <test name="Flight BAI BOG" >
    <parameter name="departure" value="BAI"></parameter>
    <parameter name="destination" value="BOG"></parameter>
    <classes><class name="demo.AutomationDemoTests" /></classes></test>
	
    <test name="Flight BOG BAI" >
    <parameter name="departure" value="BOG"></parameter>
    <parameter name="destination" value="BAI"></parameter>
    <classes><class name="demo.AutomationDemoTests" /></classes></test>
    
    <test name="Flight MVE BOG" >
    <parameter name="departure" value="MVE"></parameter>
    <parameter name="destination" value="BOG"></parameter>
    <classes><class name="demo.AutomationDemoTests" /></classes></test>
    
    <test name="Flight BOG MVE" >
    <parameter name="departure" value="BOG"></parameter>
    <parameter name="destination" value="MVE"></parameter>
    <classes><class name="demo.AutomationDemoTests" /></classes></test>
    
    <test name="Flight BAI MVE" >
    <parameter name="departure" value="BAI"></parameter>
    <parameter name="destination" value="MVE"></parameter>
    <classes><class name="demo.AutomationDemoTests" /></classes></test>
	
</suite>
